#include <dt-bindings/input/input.h>
#include "tegra30.dtsi"
#include "tegra30-cardhu-ouya-memory.dtsi"

/ {
	model = "Ouya";
	compatible = "nvidia,ouya", "nvidia,tegra30";

	/* Already disabled */
	/delete-node/ usb@7d000000;
	/delete-node/ usb-phy@7d000000;

	/* Explicitly disabled */
	/delete-node/ pcie-controller@00003000;
	/delete-node/ i2c@7000c000; /* panelddc */
	/delete-node/ panel;
	/delete-node/ backlight;
	/delete-node/ isl29029@44;

	/* FIXME: broken */

	aliases {
		rtc0 = "/rtc@7000e000";
		rtc1 = "/i2c@7000d000/tps65911@2d";
		serial0 = &uartd;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a9";
			reg = <0>;
			next-level-cache = <&L2>;
			operating-points = <
			/* kHz    uV */
/*			1700000  1350000 
			1600000  1200000
			1500000  1200000
			1400000  1200000
			1300000  1200000
			1150000  1200000*/
			1000000  1200000
/*			910000  1200000
			550000  950000
*/			>;
			clock-latency = <65536>; /* two CLK32 periods */
			#cooling-cells = <2>;
			cooling-min-level = <0>;
			cooling-max-level = <2>;
			clocks = <&tegra_car TEGRA30_CLK_CCLK_G>,
				 <&tegra_car TEGRA30_CLK_CCLK_LP>,
				 <&tegra_car TEGRA30_CLK_PLL_X>,
				 <&tegra_car TEGRA30_CLK_PLL_P>;
			clock-names = "cpu_g", "cpu_lp", "pll_x", "pll_p";

		};
		cpu@1 {
			next-level-cache = <&L2>;
		};

		cpu@2 {
			next-level-cache = <&L2>;
		};

		cpu@3 {
			next-level-cache = <&L2>;
		};
	};

	L2: cache-controller@50043000 {
	};

	smmu {
		compatible = "nvidia,tegra30-smmu";
		reg = <0x7000f010 0x02c
		       0x7000f1f0 0x010
		       0x7000f228 0x05c>;
		nvidia,#asids = <4>;		/* # of ASIDs */
		dma-window = <0 0x40000000>;	/* IOVA start & length */
		nvidia,ahb = <&ahb>;
	};

	memory {
		reg = <0x80000000 0x3fe00000>;
	};

	firmware {
		trusted-foundations {
			compatible = "tlm,trusted-foundations";
			tlm,version-major = <0x2>;
			tlm,version-minor = <0x8>;
		};
	};

	host1x@50000000 {
		dc@54200000 {
			rgb {
				status = "disabled";
				/delete-property/ nvidia,panel;
			};
		};

		hdmi@54280000 {
			status = "okay";
			nvidia,hpd-gpio = <&gpio TEGRA_GPIO(N, 7) GPIO_ACTIVE_HIGH>;
			nvidia,ddc-i2c-bus = <&hdmi_ddc>;
		};
	};

	gpio: gpio@6000d000 {
		gpio-ranges = <&pinmux 0 0 248>;
		#reset-cells = <1>;
	};

	pinmux@70000868 {
		pinctrl-names = "default";
