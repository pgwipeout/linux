+			operating-points = <
+			/* kHz    uV */
+/*			1700000  1350000 
+			1600000  1200000
+			1500000  1200000
+			1400000  1200000
+			1300000  1200000
+			1150000  1200000*/
+			1000000  1200000
+/*			910000  1200000
+			550000  950000
+*/			>;
+			clock-latency = <65536>; /* two CLK32 periods */
+			#cooling-cells = <2>;
+			cooling-min-level = <0>;
+			cooling-max-level = <2>;
+			clocks = <&tegra_car TEGRA30_CLK_CCLK_G>,
+				 <&tegra_car TEGRA30_CLK_PLL_X>,
+				 <&tegra_car TEGRA30_CLK_PLL_P>;
